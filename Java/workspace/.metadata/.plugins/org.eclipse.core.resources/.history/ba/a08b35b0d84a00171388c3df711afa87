package caesar;

import java.util.Scanner;

public class caesar {

	
	
	public String Cipher(String ToChange, int ChangeNumber){
		
		char[] ch=ToChange.toCharArray();
		
		
		for (int i = 0; i < ch.length; i++) {
			
		
		System.out.print(ch[i] + " ");
		
		}
		String finalProduct = "";
		
		return finalProduct;
		
		
	}

}
