package com.skilldistillery.Bingo;



public class Card {

	private CardSpot[][] newCard = new CardSpot[5][5];
	
	
	
	public Card(CardSpot[][] newCard) {
		super();
		this.newCard = newCard;
	}

	public CardSpot[][] getNewCard() {
		return newCard;
	}

	public void setNewCard(CardSpot[][] newCard) {
		this.newCard = newCard;
	}

	
	
	
	
	
	
}
