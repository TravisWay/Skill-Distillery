package Jets;

public class Boeing747 extends Jets {

	public Boeing747(String model,int speed, int range, int capacity) {
		super(model, speed, range, capacity);
		setCapacity(524);
		setModel("Boeing 747");
		setRange(9200);
		setSpeed(570);
		// TODO Auto-generated constructor stub
	}

	
	void DoMath() {
		float Mach = (float) (getSpeed()/761.21);
		System.out.println("Mach " + Mach);
		
	}
}
