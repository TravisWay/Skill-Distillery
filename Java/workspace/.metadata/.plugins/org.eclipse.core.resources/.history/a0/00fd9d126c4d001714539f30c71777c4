package Jets;

public class Hangar {

	public Jet[] jets;

	
	public Hangar(int size) {
		super();
		Jet[] jets = new Jet[size];
		setJets(jets);
	}

	public void addJet(Jet j1) {
		for (Jet jet : jets) {
			if (jet == null) {
				jet = j1;

			}
		}

	}

	public Jet[] getJets() {
		return jets;
	}

	public void setJets(Jet[] jets) {
		this.jets = jets;
	}

	
}