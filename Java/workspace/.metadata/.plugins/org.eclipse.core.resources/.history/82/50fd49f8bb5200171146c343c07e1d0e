package com.skilldistillery.Bingo;



public class Card {

	private CardSpot newCardSpot; 
	private CardSpot[][] newCard;
	
	
	public Card(int number,int numbertwo) {
		super();
		this.newCard = newCardSpot[number][numbertwo];
	}

	public CardSpot[][] getNewCard() {
		return newCard;
	}

	public void setNewCard(CardSpot[][] newCard) {
		this.newCard = newCard;
	}

	
	
	
	
	
	
}
