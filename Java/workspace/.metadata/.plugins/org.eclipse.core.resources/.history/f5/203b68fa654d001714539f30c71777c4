package Jets;

public class SR71 extends Jets {

	public SR71(String model, float speed, float range, int capacity) {
		super(model, speed, range, capacity);
		
		setCapacity(2);
		setModel("SR71");
		setRange(3355);
		setSpeed(2200);
		
	}

	
	void DoMath() {
		float Mach = (float) (getSpeed()/761.21);
		System.out.println("Mach " + Mach);
		
	}

}
