package Casino;

import Deck.*;
import Player.*;

public class Table {

	Deck deck = new Deck(52);
	public static void main(String[] args) {
		for (Card card : deck) {
        System.out.println(card);
      }
      Collections.shuffle(deck);
      for (Card card : deck) {
          System.out.println(card);
      }
	}
	
}
