public class InitTest{
  
   public static void main(String[] args) throws Exception{
	   int i = 1, j = 10;
	   do {
	      if (i++ > --j) continue;
	   } while (i < 5);
	   System.out.println("i=" + i + " j=" + j);
	}

    //private static String sM1(String s){
       //System.out.println(s);  return s;
    }
}
