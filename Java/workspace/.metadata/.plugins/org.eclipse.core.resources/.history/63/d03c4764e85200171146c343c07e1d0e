package Player;

import java.util.List;

import Deck.Card;

public abstract class Player {
	
	public List<Card> hand;

	public List<Card> getHand() {
		return hand;
	}

	public void setHand(List<Card> hand) {
		this.hand = hand;
	}

	public abstract Card take(List<Card> deck);
	
	public abstract void stay();
}
